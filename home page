#HOME PAGE 
import tkinter as tk

root = tk.Tk()
root.geometry("600x600")
root.title("Exercise Planner")

title_text = tk.Label(root, text="Exercise Planner", font=("Arial", 12,"bold"))
title_text.pack(pady=5)

exercise_button = tk.Button(root, text="Exercise List", font=50)
exercise_button.pack(pady=5)

tracker_button = tk.Button(root, text="Past Exercises", font=50)
tracker_button.pack(pady=5)

root.mainloop()
